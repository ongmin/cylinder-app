{
  "name": "cylinder-app",
  "version": "1.0.0",
  "description": "Video and chat room",
  "main": "index.jsx",
  "scripts": {
    "start": "npm run build && npm run start-server",
    "start-server": "node dist",
    "start-dev": "npm run build && webpack-dev-server --content-base dist",
    "build": "npm run clean && mkdir dist && npm run build-html && npm run build-webpack",
    "build-html": "cp app/index.html dist/index.html",
    "build-webpack": "webpack",
    "clean": "rm -rf dist",
    "test": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caalberts/cylinder-app.git"
  },
  "keywords": [
    "video",
    "chat",
    "react",
    "socket.io"
  ],
  "author": "Albert Salim (@caalberts), Min Ong (@ongmin)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/caalberts/cylinder-app/issues"
  },
  "homepage": "https://github.com/caalberts/cylinder-app#readme",
  "devDependencies": {
    "standard": "^5.4.1",
    "babel": "^6.3.26",
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.23.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
