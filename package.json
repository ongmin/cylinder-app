{
  "name": "cylinder-app",
  "version": "1.0.0",
  "description": "Video and chat room",
  "main": "index.jsx",
  "scripts": {
    "start": "npm run build && npm run start-server",
    "start-server": "node dist_server/server.js",
    "build": "npm run clean && mkdir dist && npm run build-html && npm run build-css && npm run build-webpack && npm run build-server",
    "build-css": "cp app/main.css dist/bundle.css",
    "build-html": "cp app/index.html dist/index.html",
    "build-webpack": "webpack",
    "build-server": "babel server -d dist_server",
    "clean": "rm -rf dist && rm -rf dist_server",
    "watch": "npm run clean && mkdir dist && npm run watch-css & npm run watch-html & npm run watch-webpack & npm run watch-server",
    "watch-webpack": "webpack --watch",
    "watch-html": "npm run build-html && watch-spawn -p 'app/index.html' npm run build-html",
    "watch-css": "npm run build-css && watch-spawn -p 'app/main.css' npm run build-css",
    "watch-server": "nodemon --exec babel-node server/server.js",
    "test": "standard",
    "test-with-mocha": "standard && mocha --compilers js:babel-register --recursive",
    "test:cover": "babel-node node_modules/.bin/babel-istanbul cover _mocha",
    "codecov": "cat ./coverage/coverage.json | ./node_modules/codecov.io/bin/codecov.io.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caalberts/cylinder-app.git"
  },
  "keywords": [
    "video",
    "chat",
    "react",
    "socket.io"
  ],
  "author": "Albert Salim (@caalberts), Min Ong (@ongmin)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/caalberts/cylinder-app/issues"
  },
  "homepage": "https://github.com/caalberts/cylinder-app#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "codecov.io": "^0.1.6",
    "enzyme": "^1.4.1",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.0.0",
    "nodemon": "^1.8.1",
    "react-addons-test-utils": "^0.14.7",
    "standard": "^5.4.1",
    "supertest": "^1.1.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "auth0-lock": "^8.2.2",
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.6",
    "babel-istanbul": "^0.6.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.4.3",
    "body-parser": "^1.14.2",
    "bootstrap": "^3.3.5",
    "browserify": "^11.0.1",
    "cors": "^2.7.1",
    "css-loader": "^0.23.1",
    "debounce": "^1.0.0",
    "express": "^4.13.4",
    "express-jwt": "^3.3.0",
    "flux": "^2.0.3",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-connect": "^2.2.0",
    "gulp-open": "^1.0.0",
    "jquery": "^2.1.4",
    "keymirror": "^0.1.1",
    "lodash": "^4.2.0",
    "mongoose": "^4.4.0",
    "object-assign": "^3.0.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.7",
    "react-geosuggest": "^1.14.0",
    "react-router": "^0.13.5",
    "reactify": "^1.1.1",
    "slug": "^0.9.1",
    "standard": "^5.4.1",
    "style-loader": "^0.13.0",
    "toastr": "^2.1.0",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^1.12.12",
    "youtube-search-api": "^0.5.3"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
