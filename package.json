{
  "name": "cylinder-app",
  "version": "1.0.0",
  "description": "Video and chat room",
  "main": "index.jsx",
  "scripts": {
    "start": "gulp",
    "start-build": "npm run build && npm run start-server",
    "start-server": "node dist",
    "start-dev": "npm run build && webpack-dev-server --content-base dist",
    "build": "npm run clean && mkdir dist && npm run build-html && npm run build-webpack",
    "build-html": "cp app/index.html dist/index.html",
    "build-webpack": "webpack",
    "clean": "rm -rf dist",
    "test": "standard && mocha --compilers js:babel-register",
    "test:cover": "babel-node node_modules/.bin/babel-istanbul cover _mocha",
    "codecov": "cat ./coverage/coverage.json | ./node_modules/codecov.io/bin/codecov.io.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caalberts/cylinder-app.git"
  },
  "keywords": [
    "video",
    "chat",
    "react",
    "socket.io"
  ],
  "author": "Albert Salim (@caalberts), Min Ong (@ongmin)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/caalberts/cylinder-app/issues"
  },
  "homepage": "https://github.com/caalberts/cylinder-app#readme",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.6",
    "babel-istanbul": "^0.6.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.4.3",
    "chai": "^3.5.0",
    "codecov.io": "^0.1.6",
    "css-loader": "^0.23.1",
    "enzyme": "^1.4.1",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.0.0",
    "standard": "^5.4.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
